<script type="text/javascript">
    var defaultSet = false;
    jQuery('#country-select').ddslick({
        width: 80,
        onSelected: function(data){
            var ddData = jQuery('#country-select').data('ddslick');

            if (defaultSet && ddData.selectedData.value != 'US') {
                var dt = new Date(), expiryTime = dt.setTime( dt.getTime() + 60*60*24 );
                //Mage.Cookies.set("prefCCode", ddData.selectedData.value);
                document.cookie = "prefCCode=" + escape(ddData.selectedData.value) + "; path=/; domain=dollarshaveclub.com";

                //callback function: do something with selectedData;
                location.href='http://' + (data.selectedData.value == 'US' ? 'www' : data.selectedData.value) + '.dollarshaveclub.com';
            }
            defaultSet = true;
        }
    });
    jQuery('#country-select').ddslick('select', {index: 0 });

    // now hide the drop down option for the current country
    jQuery('.dd-option-value[value=US]').closest('li').hide();
</script>
