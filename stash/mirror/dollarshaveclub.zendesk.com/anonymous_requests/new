        $j.ajax({
          url: '/widgets/async.json?user_id=' + currentUser.id + '&v=' + cacheBuster,
          data: {"controller_name":"anonymous","action_name":"new","action_method":"get"},
          success: Widget.asyncInsertion
        });
      });
