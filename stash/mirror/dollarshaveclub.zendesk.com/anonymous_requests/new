  $j('#hidden-file-input').mouseover(function(e){ $j('#attach_link').addClass('attach_link_selected'); $j(this).css('cursor', 'pointer'); });
  $j('#hidden-file-input').mouseout(function(e){ $j('#attach_link').removeClass('attach_link_selected');});

  $j("#attach_link").mousemove(function(e) {
    var input = $j(this).find("#hidden-file-input");
    input.css({
      left: e.pageX- $j(this).offset().left - input.width() + 50,
      top: e.pageY - $j(this).offset().top -10
    })
  });
